{
    "name": "daniel-km/omeka-s-module-block-plus",
    "type": "omeka-s-module",
    "description": "Module for Omeka S: Add page models, groups of blocks and new blocks for editorial site pages (image gallery, D3 graph, mirror page, search form, showcase, exhibits, etc.) and resource pages (title, thumbnail, previous/next, etc.).",
    "keywords": [
        "Omeka S",
        "Omeka S module",
        "block",
        "page",
        "theme"
    ],
    "license": "CECILL-2.1",
    "homepage": "https://gitlab.com/Daniel-KM/Omeka-S-module-BlockPlus",
    "authors": [
        {
            "name": "Daniel Berthereau",
            "email": "Daniel.packagist@Berthereau.net"
        }
    ],
    "support": {
        "issues": "https://gitlab.com/Daniel-KM/Omeka-S-module-BlockPlus/-/issues",
        "forum": "https://forum.omeka.org/"
    },
    "_require (not set until Omeka S merges PR #2412)": {
        "omeka/omeka-s": "^4.1",
        "daniel-km/omeka-s-module-common": "*"
    },
    "require": {
        "sempia/external-assets": "^1.0"
    },
    "replace": {
        "jsor/jcarousel": "*"
    },
    "autoload": {
        "psr-4": {
            "BlockPlus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlockPlusTest\\": "tests/BlockPlusTest/"
        }
    },
    "extra": {
        "installer-name": "BlockPlus",
        "external-assets": {
            "asset/vendor/ckeditor-footnotes/": "https://github.com/Daniel-KM/Omeka-S-module-BlockPlus/releases/download/3.4.33/CKEditorFootnotes.1.2.0-fix-blockplus.tar.gz",
            "asset/vendor/thumbnail-grid-expanding-preview/": "https://github.com/Daniel-KM/ThumbnailGridExpandingPreview/archive/refs/tags/0.3.0.tar.gz",
            "asset/vendor/d3/d3.min.js": "https://unpkg.com/d3@7.9.0/dist/d3.min.js",
            "asset/vendor/jcarousel/": "https://github.com/jsor/jcarousel/archive/refs/heads/master.tar.gz",
            "asset/vendor/slick/": "https://github.com/kenwheeler/slick/archive/refs/heads/master.tar.gz",
            "asset/vendor/jslists/": "https://github.com/TheTechy/jslists/archive/refs/heads/master.tar.gz"
        }
    },
    "config": {
        "allow-plugins": {
            "sempia/external-assets": true
        }
    }
}
